public class Main
{
	public static void main(String[] args) {
	    int a = 0;
	    boolean kondisi = true;
	    
	    do{
	        a++;
	        System.out.println("Do while ke - " + a);
	        
	        if (a == 20){
	            kondisi = false;
	            
	        }
	    }while(kondisi);
	    
	    System.out.println("Akhir program");
}
}
	
